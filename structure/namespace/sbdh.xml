			<DataType>xs:anyURI</DataType>
		</Element>
		<Element name="Description">
			<DataType>xs:string</DataType>
		</Element>
		<Element name="Language">
			<DataType>xs:string</DataType>
		</Element>
		<Element name="LanguageCode">
