<?xml version="1.0" encoding="UTF-8"?>
<CatalogueRequest xmlns="urn:oasis:names:specification:ubl:schema:xsd:CatalogueRequest-2"
	xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2"
	xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2">
	<cbc:CustomizationID>urn:fdc:peppol.eu:logistics:trns:advanced_despatch_advice:1</cbc:CustomizationID>
	<cbc:ProfileID>urn:fdc:peppol.eu:logistics:bis:despatch_advice_only:1</cbc:ProfileID>
	<cbc:ID>5658999</cbc:ID>
	
	<!-- tag::example[] -->
	<!-- Example of use in the PreAward Catalogue request message. -->

	<cac:AdditionalItemProperty>
		<cbc:Name>Colour</cbc:Name>
		<cbc:Value>Red</cbc:Value>
	</cac:AdditionalItemProperty>

	<cac:AdditionalItemProperty>
		<cbc:Name>NutritionProtein</cbc:Name>
		<cbc:Value>NA</cbc:Value>
		<cbc:ValueQuantity unitCode="GRM" unitCodeListID="UNECERec20">2.5</cbc:ValueQuantity>
	</cac:AdditionalItemProperty>

	<cac:AdditionalItemProperty>
		<cbc:Name>GeneticallyModified</cbc:Name>
		<cbc:Value>True</cbc:Value>
	</cac:AdditionalItemProperty>

	
	<!-- Here an example of a Property that is defined with a Name-code in a codelist. -->

	<cac:AdditionalItemProperty>
		   <cbc:Name>Thermal transmittance</cbc:Name>
		   <cbc:NameCode listID="EN14351-1">ThermalTransmittance</cbc:NameCode>
		   <cbc:Value>NA</cbc:Value>
		   <cac:ItemPropertyRange>
			      <cbc:MaximumValue>1.4</cbc:MaximumValue>
		</cac:ItemPropertyRange>
	</cac:AdditionalItemProperty>


	<!-- And here an example of a Property defined by an ID in a DataTemplate with link. -->

	<cac:AdditionalItemProperty>
		<cbc:ID schemeID="ISO22057" schemeVersionID="2022" schemeDataURI="https://define.cobuilder.no/77e416eb-a363-4258-a04e- 
 			171d843a6460/2022/">77e416eb-a363-4258-a04e-171d843a6460</cbc:ID>
		   <cbc:Name>xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx</cbc:Name>
		   <cbc:Value>NA</cbc:Value>
		   <cbc:ValueQuantity unitCode="MTR">4.30</cbc:ValueQuantity>
	</cac:AdditionalItemProperty>
	<!-- end::example[] -->

</CatalogueRequest>
