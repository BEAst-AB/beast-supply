<?xml version="1.0" encoding="UTF-8"?>
<!--BII2-T19-R006-->
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0"
	configuration="peppolbis-t19-base-3.0">
	<assert>
		<description>Verify period end date is later than or equal to start date</description>
		<scope>PEPPOL-T19-R001</scope>
	</assert>
	
	<test id="1">
		<assert>
			<success>PEPPOL-T19-R001</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">
			
				<cac:ValidityPeriod>
					<cbc:StartDate>2018-09-01</cbc:StartDate>
					<cbc:EndDate>2019-08-31</cbc:EndDate>
				</cac:ValidityPeriod>

		</Catalogue>
	</test>


	<test id="2">
		<assert>
			<success>PEPPOL-T19-R001</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">
			
				<cac:ValidityPeriod>
					<cbc:StartDate>2018-09-01</cbc:StartDate>
					<cbc:EndDate>2018-09-01</cbc:EndDate>
				</cac:ValidityPeriod>

		</Catalogue>
	</test>


	<test id="3">
		<assert>
			<success>PEPPOL-T19-R001</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">
			
				<cac:ValidityPeriod>
					<cbc:StartDate>2018-09-01</cbc:StartDate>
				</cac:ValidityPeriod>

		</Catalogue>
	</test>

	<test id="4">
		<assert>
			<success>PEPPOL-T19-R001</success>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
			xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
			xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">
			
			<cac:ValidityPeriod>
				<cbc:EndDate>2018-09-01</cbc:EndDate>
			</cac:ValidityPeriod>
			
		</Catalogue>
	</test>
	
	<test id="5">
		<assert>
			<error>PEPPOL-T19-R001</error>
		</assert>
		<Catalogue xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">
			
				<cac:ValidityPeriod>
					<cbc:StartDate>2018-09-01</cbc:StartDate>
					<cbc:EndDate>2018-08-01</cbc:EndDate>
				</cac:ValidityPeriod>

		</Catalogue>
	</test>


	
</testSet>